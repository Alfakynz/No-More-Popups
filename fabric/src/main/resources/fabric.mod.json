{
    "schemaVersion": 1,
    "id": "nomorepopups",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${description}",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "https://github.com/Alfakynz/No-More-Popups",
        "sources": "https://github.com/Alfakynz/No-More-Popups",
        "issues": "https://github.com/Alfakynz/No-More-Popups/issues"
    },
    "license": "${license}",
    "icon": "assets/${mod_id}/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": ["com.alfakynz.nomorepopups.NoMorePopupsFabric"],
        "modmenu": ["com.alfakynz.nomorepopups.config.ModMenuIntegration"]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "minecraft": ">=${minecraft_version}",
        "java": ">=17"
    },
    "suggests": {
        "fabric": "*",
        "modmenu": "*",
        "cloth-config": "*"
    },
    "custom": {
      "modmenu": {
        "links": {
          "modmenu.discord": "https://discord.gg/TcmUQHZ5U4",
          "modmenu.wiki": "https://github.com/Alfakynz/No-More-Popups/wiki"
        }
      }
    }
}
